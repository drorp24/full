  "devDependencies": {},
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "REACT_APP_SERVER=true REACT_APP_SSR=true react-scripts build --profile ",
    "buildNoSsr": "REACT_APP_SERVER=true REACT_APP_SSR=false react-scripts build --ptofile ",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "noServer": "REACT_APP_SERVER=false REACT_APP_SSR=false react-scripts start",
    "server": "REACT_APP_SERVER=true node server/bootstrap.js dotenv_config_path=.env.production",
    "serverSsr": "REACT_APP_SSR=true req=index.js yarn server",
    "serverNoSsr": "REACT_APP_SSR=false req=indexNoSsr.js yarn server",
    "graphqlDev": "cd ../graphql && npm run startDev --scripts-prepend-node-path",
    "noServerGraphqlDev": "REACT_APP_SERVER=false REACT_APP_SSR=false yarn graphqlDev",
    "serverGraphqlDev": "REACT_APP_SERVER=true yarn graphqlDev",
    "noServerAndGraphqlDev": "concurrently \"yarn noServer\" \"yarn noServerGraphqlDev\"",
    "serverSsrDevAndGraphqlDev": "concurrently \"REACT_APP_SERVER=true REACT_APP_SSR=true yarn build  && yarn analyze && yarn serverSsr\" \"REACT_APP_SERVER=true REACT_APP_SSR=true yarn graphqlDev\"",
    "serverNoSsrDevAndGraphqlDev": "concurrently \"REACT_APP_SERVER=true REACT_APP_SSR=false yarn buildNoSsr && yarn serverNoSsr\" \"REACT_APP_SERVER=true REACT_APP_SSR=false yarn graphqlDev\"",
    "start": "yarn serverSsr"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
